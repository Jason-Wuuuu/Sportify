<h1 class="centertitle">All Reserved Venue</h1>
{{#if isempty}}
<p>No, Venue is Booked yet.</p>
{{else}}
<div class="centertitle">
    <p>==============================================================================</p>
    <table>
        <tr>
            <th>Sport</th>
            <th>Venue Name</th>
            <th>Booking Date</th>
            <th>Timing</th>
            <th>Address</th>
            <th>Description</th>
            <th>Price</th>
            <th>Capacity</th>

        </tr>

        {{#each venueList}}


        <tr>

            <td>{{this.sportName}}</td>
            <td>{{this.sportPlaceName}}</td>
            <td>{{this.Date}}</td>
            <td> {{this.slotName}}</td>
            <td>{{this.address}}</td>
            <td>{{this.description}}</td>
            <td>{{this.price}}</td>
            <td>{{this.capacity}}</td>


        </tr>
        {{/each}}
    </table>
</div>
{{/if}}

<h1 class="centertitle">Past Reserved Venue</h1>
{{#if isemptyold}}
<p>No,Venue is Booked in past.</p>
{{else}}
<form id="myForm">
    <div class="centertitle">
        <p>==============================================================================</p>
        <table>
            <tr>
                <th>Sport</th>
                <th>Venue Name</th>
                <th>Booking Date</th>
                <th>Timing</th>
                <th>Address</th>
                <th>Description</th>
                <th>Price</th>
                <th>Capacity</th>

            </tr>

            {{#each venuelistold}}
            <tr>
                <td>{{this.sportName}}</td>
                <td>{{this.sportPlaceName}}</td>
                <td>{{this.Date}}</td>
                <td> {{this.slotName}}</td>
                <td>{{this.address}}</td>
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.capacity}}</td>

            </tr>
            {{/each}}
        </table>
    </div>
</form>
{{/if}}