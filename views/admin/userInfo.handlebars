<header>
    <nav class="navbar">
        <a href="/admin/users">Back</a>
    </nav>
</header>

<h1>{{title}}</h1>

<ul>
    <li>First Name: {{firstName}}</li>
    <li>Last Name: {{lastName}}</li>
    <li>Email: {{email}}</li>
    <li id="gender" value="{{gender}}">Gender: {{gender}}</li>
    <li>Date Of Birth: {{dateOfBirth}}</li>
    <li>Contact Number: {{contactNumber}}</li>
</ul>

<form action="/admin/users/remove/{{id}}" method="get">
    <label for="remove">Remove User:</label>
    <button type="submit" id="remove">Remove</button>
</form>